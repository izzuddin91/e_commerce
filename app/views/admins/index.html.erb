<h1>Admin log in here</h1>
<h1>Sign up</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@admin) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
<br>
      <%= f.label :email %>
      <%= f.email_field :email_address %>
<br>
      <%= f.label :password %>
      <%= f.password_field :password %>
<br>
      <%= f.submit "Create my account", class: "btn btn-primary" %>
    <% end %>
  </div>
<a href="/login"><p>if already have account, log in here:</p></a>
  

</div>
